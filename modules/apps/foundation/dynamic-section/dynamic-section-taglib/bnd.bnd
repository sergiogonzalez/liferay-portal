Bundle-Name: Liferay Dynamic Section Taglib
Bundle-SymbolicName: com.liferay.dynamic.section.taglib
Bundle-Version: 1.0.0
Export-Package:\
	com.liferay.dynamic.section.taglib,\
	com.liferay.dynamic.section.taglib.servlet.taglib

Provide-Capability:\
	osgi.extender;\
		osgi.extender="jsp.taglib";\
		uri="http://liferay.com/tld/dynamic-section";\
		version:Version="${Bundle-Version}"
Web-ContextPath: /dynamic-section-taglib