<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.1.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_1_0.dtd">

<service-builder auto-import-default-references="false" auto-namespace-tables="false" package-path="com.liferay.document.library.google.drive">
	<namespace>GoogleDrive</namespace>
	<entity local-service="true" name="GoogleDriveEntry" remote-service="false" trash-enabled="false" uuid="false">

		<!-- PK fields -->

		<column name="entryId" primary="true" type="long" />

		<!-- Group instance -->

		<column name="groupId" type="long" />

		<!-- Audit fields -->

		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" uad-anonymize-field-name="fullName" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->

		<column name="fileEntryId" type="long" uad-nonanomizable="true" />
		<column name="googleDriveId" type="String" uad-nonanomizable="true" />

		<!-- Finder methods -->

		<finder name="G_F" return-type="GoogleDriveEntry">
			<finder-column name="groupId" />
			<finder-column name="fileEntryId" />
		</finder>

		<!-- References -->

		<reference entity="User" package-path="com.liferay.portal" />
	</entity>
</service-builder>